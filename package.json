{
  "name": "ag-react-app-test-01",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "env": {
      "es6" : true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
            "jsx": "true"
        }
    },    
    "globals": {
      "it" : "readable",
      "fetch" : "readable",
      "console" : "readable",
      "URL" : "readable",
      "navigator" : "readable",
      "process" : "readable",
      "window" : "readable",
      "document" : "readable",
      "ReactDOM" : "readable"
    },
    "rules": {
      "semi": "off",
      "no-console": "off",
      "no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
